{
    "name": "scss-bundle",
    "version": "1.0.0",
    "description": "Bundling SCSS files to one bundled file.",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "scripts": {
        "build": "tsc -p .",
        "watch": "tsc -p . -w",
        "start": "node dist/cli/main.js"
    },
    "keywords": [
        "scss",
        "bundle",
        "sass",
        "node-sass"
    ],
    "files": [
        "dist",
        "**/*.md",
        "*.js",
        "!*.config.js"
    ],
    "author": "simplrjs <simplr@quatrodev.com> (https://github.com/simplrjs)",
    "license": "MIT",
    "devDependencies": {
        "@reactway/eslint-config": "^1.0.0-alpha.2",
        "eslint": "^6.1.0",
        "typescript": "^3.5.3",
        "@simplrjs/test-generator-cli": "^0.1.3",
        "node-sass": "^4.12.0"
    },
    "peerDependencies": {
        "node-sass": "^4.12.0"
    },
    "dependencies": {
        "@types/archy": "^0.0.31",
        "@types/debug": "^4.1.4",
        "@types/fs-extra": "^8.0.0",
        "@types/glob": "^7.1.1",
        "@types/lodash.debounce": "^4.0.6",
        "@types/node-sass": "^4.11.0",
        "archy": "^1.0.0",
        "chalk": "^2.4.2",
        "chokidar": "^3.0.2",
        "commander": "^2.20.0",
        "fs-extra": "^8.1.0",
        "globs": "^0.1.4",
        "lodash.debounce": "^4.0.8",
        "loglevel": "^1.6.3",
        "loglevel-plugin-prefix": "^0.8.4",
        "pretty-bytes": "^5.3.0"
    }
}
